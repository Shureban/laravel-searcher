{
    "name": "shureban/laravel-searcher",
    "description": "Laravel SDK for searching data in DB using requests",
    "keywords": [
        "searcher",
        "models",
        "requests",
        "search",
        "shureban"
    ],
    "type": "laravel-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Shureban",
            "email": "Shureban@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "version": "2.4.0",
    "autoload": {
        "psr-4": {
            "Shureban\\LaravelSearcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shureban\\LaravelSearcher\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Shureban\\LaravelSearcher\\SearcherServiceProvider"
            ]
        }
    }
}
